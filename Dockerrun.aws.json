{
	"AWSEBDockerrunVersion": 2,
	"containerDefinitions": [
		{
			"name": "frontend",
			"image": "sunwoo005/docker-frontend",
			"hostname": "frontend",
			"essential": false,
			"memory": 128
		},
		{
			"name": "backend",
			"image": "sunwoo005/docker-backend",
			"hostname": "backend",
			"essential": false,
			"memory": 128
		},
		{
			"name": "nginx",
			"image": "sunwoo005/docker-nginx",
			"hostname": "nginx",
			"essential": true,
			"portsMappings": [
				{
					"hostPort": 80,
					"containerPort": 80
				}
			],
			"links": ["frontend", "backend"],
			"memory": 128
		}
	]
}
